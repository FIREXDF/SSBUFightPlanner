{
    "productName": "FightPlanner Canary",
    "appId": "com.fightplanner.canary",
    "directories": {
      "output": "dist-canary"
    },
    "files": [
      "!dist/**"
    ],
    "extraResources": [
        {
            "from": "./build",
            "to": "build",
            "filter": [
                "**/*"
            ]
        },
        {
            "from": "src/resources",
            "to": "src/resources",
            "filter": ["**/*"]
        }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "FIREXDF",
        "repo": "FightPlannerEarly"
      }
    ]
  }
  